<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Usuario_no_modificable</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(CONTAINS( $Profile.Name , &quot;Admin&quot;))</errorConditionFormula>
    <errorMessage>No tienes los permisos para modificar el usuario.</errorMessage>
</ValidationRule>
