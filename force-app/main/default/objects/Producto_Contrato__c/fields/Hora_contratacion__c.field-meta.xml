<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Hora_contratacion__c</fullName>
    <description>Recoge la hora de contratación para el uso en informes</description>
    <externalId>false</externalId>
    <formula>HOUR(TIMEVALUE(CreatedDate + ( $Setup.Correccion_Hora__c.Horas_Add__c /24)))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Hora contratación</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
